<%- include("partials/header") %>
<form action="/compose" method="post">
  <h1>Compose</h1>
  <div class="form-group">
    <label for="postTitle" class="">Title</label>
    <input class="form-control" type="text" id="postTitle" name="postTitle" />
  </div>
  <div class="form-group">
    <label for="postContent" class="">Post</label>
    <textarea class="form-control" id="postContent" name="postContent" rows="6"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include("partials/footer") %>
